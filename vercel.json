{
  "routes": [
    {
      "src": "/gallery",
      "dest": "/index.html"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|mp4|mp3|json))",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html"
    }
  ]
}
